<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>MovieServlet</servlet-name>
    <servlet-class>com.left.selvlet.MovieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteMovieServlet</servlet-name>
    <servlet-class>com.left.selvlet.DeleteMovieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateMovieServlet</servlet-name>
    <servlet-class>com.left.selvlet.UpdateMovieServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TransportServlet</servlet-name>
    <servlet-class>com.left.selvlet.TransportServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.left.selvlet.AddServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>MovieServlet</servlet-name>
    <url-pattern>/servlet/MovieServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteMovieServlet</servlet-name>
    <url-pattern>/servlet/DeleteMovieServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateMovieServlet</servlet-name>
    <url-pattern>/servlet/UpdateMovieServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TransportServlet</servlet-name>
    <url-pattern>/servlet/TransportServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/servlet/AddServlet</url-pattern>
  </servlet-mapping>

</web-app>